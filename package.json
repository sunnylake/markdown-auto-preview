{
  "name": "markdown-auto-preview",
  "displayName": "Auto Open Markdown Preview",
  "publisher": "effcode",
  "version": "0.0.1",
  "description": "automatically open the preview, when you open a markdown file",
  "engines": {
    "vscode": "^1.84.0"
  },
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "icon": "imgs/icon.png",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/vscode": "^1.84.0",
    "vsce": "^2.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sunnylake/markdown-auto-preview.git"
  }
}
